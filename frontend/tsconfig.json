{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "esnext", 
    "module": "esnext",
    "jsx": "react-jsx", 
    "moduleResolution": "node", 
    "resolveJsonModule": true,
    "allowJs": true, 
    "skipLibCheck": true, 
    "strict": false, 
    "noEmit": true,
    "isolatedModules": false,
    "allowImportingTsExtensions": true,
    "forceConsistentCasingInFileNames": false,
    "noImplicitAny": false 
  },
  "include": ["src"]
}
